[application]

config/name="Godot Breakout"
config/description="A Breakout Clone made in Godot Engine 3.5.3"
run/main_scene="res://src/scene/Game.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="godot_breakout"
boot_splash/image="res://src/assets/texture/splash.png"
